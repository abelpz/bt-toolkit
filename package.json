{
  "name": "@bt-toolkit/source",
  "version": "0.0.0",
  "license": "MIT",
  "bin": {
    "btt": "./bin/btt.js"
  },
  "scripts": {
    "dev": "nx run-many -t serve --parallel=5",
    "build:all": "nx run-many -t build",
    "build:apps": "nx run-many -t build --projects=tag:type:app",
    "build:libs": "nx run-many -t build --projects=tag:type:lib",
    "build:affected": "nx affected -t build",
    "test:all": "nx run-many -t test",
    "test:apps": "nx run-many -t test --projects=tag:type:app",
    "test:libs": "nx run-many -t test --projects=tag:type:lib",
    "test:affected": "nx affected -t test",
    "test:e2e": "nx run-many -t e2e",
    "lint:all": "nx run-many -t lint",
    "lint:affected": "nx affected -t lint",
    "graph": "nx graph",
    "local-registry": "nx local-registry",
    "publish:local": "nx run $npm_config_name:build && cd dist/packages/$npm_config_name && npm publish --registry http://localhost:4873",
    "publish:npm": "nx run $npm_config_name:build && cd dist/packages/$npm_config_name && npm publish --access public",
    "publish:libs": "nx run-many -t build --projects=tag:type:lib && node scripts/publish-all-libs.js",
    "deploy:web": "node scripts/deploy-web-apps.js",
    "deploy:apps": "npm run build:apps && npm run deploy:web && npm run package:desktop",
    "package:desktop": "node scripts/package-desktop.js",
    "build:mobile": "node scripts/build-mobile.js",
    "docs:generate": "typedoc",
    "docs:serve": "cd docs/linked-panels && npx http-server -p 8080",
    "docs:build": "npm run docs:generate",
    "release": "nx release",
    "affected:graph": "nx affected:graph"
  },
  "private": true,
  "devDependencies": {
    "@astrojs/check": "^0.5.2",
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^6.0.2",
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "21.1.3",
    "@nx/eslint": "21.1.3",
    "@nx/eslint-plugin": "21.1.3",
    "@nx/jest": "21.1.3",
    "@nx/js": "21.1.3",
    "@nx/playwright": "21.1.3",
    "@nx/react": "^21.1.3",
    "@nx/vite": "21.1.3",
    "@nx/web": "21.1.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@swc/jest": "~0.2.36",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.0.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "ajv": "^8.0.0",
    "astro": "^4.3.5",
    "autoprefixer": "10.4.13",
    "babel-jest": "^29.7.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.1.3",
    "postcss": "8.4.38",
    "prettier": "^2.6.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwindcss": "3.4.3",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typedoc": "^0.28.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.19.0",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.0.0"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "apps/translation-tools/*",
    "packages/ui/*"
  ],
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  },
  "dependencies": {
    "@astrojs/mdx": "^3.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "chalk": "4.1.2",
    "commander": "^14.0.0",
    "immer": "^10.1.1",
    "inquirer": "8.2.6",
    "inversify": "^7.5.2",
    "react-resizable-panels": "^3.0.2",
    "react-router-dom": "6.29.0",
    "reflect-metadata": "^0.2.2",
    "zustand": "^5.0.5"
  }
}
